{
    "name": "@agent47/mcp-server",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "bin": {
        "mcp-server": "./dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "bundle": "esbuild src/http-server.ts --bundle --platform=node --outfile=dist/server.cjs --format=cjs --external:chromium-bidi/lib/cjs/bidiMapper/BidiMapper --external:chromium-bidi/lib/cjs/cdp/CdpConnection --external:playwright",
        "start": "node dist/server.cjs",
        "start:stdio": "node dist/index.js",
        "dev": "tsx src/http-server.ts",
        "dev:stdio": "tsx src/index.ts"
    },
    "dependencies": {
        "@agent47/aggregator": "*",
        "@agent47/integrations": "*",
        "@agent47/shared": "*",
        "@modelcontextprotocol/sdk": "^1.0.4",
        "@virtuals-protocol/acp-node": "^0.3.0-beta.29",
        "@coinbase/cdp-sdk": "latest",
        "cheerio": "^1.0.0-rc.12",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "node-fetch": "^3.3.2",
        "playwright": "^1.40.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "esbuild": "^0.24.0",
        "eventsource": "^3.0.7",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}